{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Emulator FPFC",
    "id": "emulator-fpfc",
    "version": "0.1.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libemulator-fpfc.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./Emulator FPFC.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.0.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.0.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4006.0.0",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    }
  ]
}